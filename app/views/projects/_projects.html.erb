  <div class="list_window">
    <div class="list_title">
      <%= image_tag("note.gif", class: "note_list_img") %>
      <span class="title">Complete the test task for Ruby Garage</span>
      <%= image_tag("title_trash.gif", class: "title_trash_list_img") %>
      <%= image_tag("title_pen.gif", class: "title_pen_list_img") %>
      <!--img src="/images/title_line.png", class="title_line_list_img"-->
    </div><!--end list_title-->
    <div class="list_addtask">
      <%= image_tag("plus.gif", class: "addtask_plus") %>
      <input class="add_field" type="text" placeholder="Start typing here to create a task...">
      <input class="deadline" type="text" placeholder="deadline">
      <a href="" class="addtask_button">Add Task</a>
    </div><!--end list_addtask-->
  </div><!--end list_window-->